import java.util.Scanner;

public class Main {
	
	public static void main (String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int HI = sc.nextInt();
		int HF = sc.nextInt();
		
		int duracao;
		if (HI < HF) {
			duracao = HF - HI;
		}
		else {
			duracao = 24 - HI+ HF;
		}
		
		System.out.println("O JOGO DUROU " + duracao + " HORA(S)");
		}
	
		
		sc.close();
		}

}